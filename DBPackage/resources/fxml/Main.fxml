<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.162" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Options">
          <items>
            <MenuItem fx:id="setMenuItem" mnemonicParsing="false" text="Settings" />
          </items>
          <items>
            <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" text="About DBPackage" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <BorderPane fx:id="mainPane" prefHeight="600.0" prefWidth="400.0" style="-fx-padding: 10;">
         <top>
            <HBox alignment="CENTER" style="-fx-spacing: 10;" BorderPane.alignment="CENTER">
               <children>
                  <Label text="URL:" />
                  <TextField HBox.hgrow="ALWAYS" fx:id="textField" prefHeight="22.0" prefWidth="400.0" />
                  <Button fx:id="searchBut" mnemonicParsing="false" text="Search">
                  	<graphic>
				        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
				          <image>
				            <Image url="/search.png" />
				          </image>
				        </ImageView>
				      </graphic>
				  </Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </BorderPane.margin>
            </HBox>
         </top>
         <bottom>
            <HBox alignment="CENTER" style="-fx-spacing: 10;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="genBut" mnemonicParsing="false" text="Generate Package">
                  	<graphic>
				        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
				          <image>
				            <Image url="/network.png" />
				          </image>
				        </ImageView>
				      </graphic>
				  </Button>
				  <Button fx:id="openBut" mnemonicParsing="false" text="Open File">
                  	<graphic>
				        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
				          <image>
				            <Image url="/file.png" />
				          </image>
				        </ImageView>
				      </graphic>
				  </Button>
				  <Button fx:id="expBut" mnemonicParsing="false" text="Expand All">
                  	<graphic>
				        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
				          <image>
				            <Image url="/plus.png" />
				          </image>
				        </ImageView>
				      </graphic>
				  </Button>
				  <Button fx:id="collBut" mnemonicParsing="false" text="Collapse All">
                  	<graphic>
				        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
				          <image>
				            <Image url="/minus.png" />
				          </image>
				        </ImageView>
				      </graphic>
				  </Button>
				  <Button fx:id="exitBut" mnemonicParsing="false" text="Exit">
                  	<graphic>
				        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
				          <image>
				            <Image url="/exit.png" />
				          </image>
				        </ImageView>
				      </graphic>
				  </Button>
               </children>
               <BorderPane.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </HBox>
         </bottom>
      </BorderPane>
  </children>
</VBox>
